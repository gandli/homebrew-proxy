{
  "default": true,
  "MD003": {
    "style": "atx"
  },
  "MD007": {
    "indent": 2
  },
  "MD013": {
    "line_length": 120,
    "code_blocks": false,
    "tables": false,
    "headings": false,
    "headers": false
  },
  "MD024": {
    "allow_different_nesting": true
  },
  "MD033": {
    "allowed_elements": [
      "details",
      "summary",
      "br",
      "kbd",
      "sub",
      "sup"
    ]
  },
  "MD041": false,
  "MD025": {
    "front_matter_title": "^\\s*title\\s*[:=]"
  },
  "MD026": {
    "punctuation": ".,;:!?"
  },
  "MD051": false
}
